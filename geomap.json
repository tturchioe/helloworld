{
  "id": "com.sap.custom.geomap",
  "version": "1.0.0",
  "name": "GeoMap",
  "description": "GeoMap widget v2",
  "newInstancePrefix": "geoMap",
  "icon": "data:image/png;base64,",
  "vendor": "sap",
  "eula": "",
  "license": "",
  "imports": [ "input-controls" ],
  "webcomponents": [
    {
      "kind": "main",
      "tag": "com-sap-custom-geomap",
      "url": "https://tturchioe.github.io/helloworld/geomap_component.js",
      "integrity": "",
      "ignoreIntegrity": true
    },
    {
      "kind": "styling",
      "tag": "com-sap-custom-geomap-aps",
      "url": "https://tturchioe.github.io/sacwidget/geomap_aps.js",
      "integrity": "",
      "ignoreIntegrity": true
    }
  ],
  "properties": {
    "metadata": {
      "type": "string",
      "description": "For internal use",
      "default": ""
    },
    "restapiurl": {
      "type": "string",
      "description": "The REST API URL",
      "default": "http://ec2-3-88-217-89.compute-1.amazonaws.com/sap/sw/geogeneric"
    },
    "name": {
      "type": "string",
      "description": "widget name",
      "default": ""
    },
    "score": {
      "type": "string",
      "description": "score",
      "default": ""
    }
  },
  "servicelevel": {
      "type": "integer",
      "description": "Electrical Service Level",
      "default": 0
  },
  "methods": {
    "getRestAPIUrl": {
      "returnType": "string",
      "description": "Get the REST API URL",
      "parameters": [      
      ],
      "body": "return this.restapiurl;"
    },
    "setRestAPIUrl": {
      "description": "Set the REST API URL",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "The new REST API URL"
        }
      ],
      "body": "this.restapiurl = value;"
    },
    "setServiceLevel": {
      "description": "Set the customer Service Level",
      "parameters": [
        {
          "name": "service",
          "type": "integer",
          "description": "ServiceLevel"
        }
      ],
      "body": "this.servicelevel = service;"
    },
    "getScore": {
      "returnType": "string",
      "description": "Get the score",
      "parameters": [
      ],
      "body": "return this.score;"
    },
    "setScore": {
      "description": "Set the score",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "The new score"
        }
      ],
      "body": "this.score = value;"
    },
    "getName": {
      "returnType": "string",
      "description": "Get widget name",
      "parameters": [
        
      ],
      "body": "return this.name;"
    },
    "setName": {
      "description": "Set widget name",
      "parameters": [
        {
          "name": "value",
          "type": "string",
          "description": "The new widget name"
        }
      ],
      "body": "this.name = value;"
    }
  },
  "events": {
    "onStart": {
      "description": "onStart events trigger"
    }
  }
}